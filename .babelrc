{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": 3,
        "targets": {
          "chrome": 60
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "styled-jsx/babel",
    "@babel/plugin-transform-arrow-functions",
    "@babel/plugin-transform-classes",
    "@babel/plugin-transform-runtime"
  ]
}
